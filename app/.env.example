# Env
ENVIRONMENT=development

# Secrets (DO NOT COMMIT ACTUAL KEYS)
JWT_SECRET=your_jwt_secret
FERNET_KEY=your_fernet_key

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Database
DB_ENGINE=postgres
POSTGRES_URL=postgresql+asyncpg://postgres:postgres@db:5432/hyphaeos
SQLITE_URL=sqlite+aiosqlite:///./data/hyphaeos.db

# Redis
REDIS_URL=redis://localhost:6379

# Memory
MEMORY_MODE=redis

# Email
EMAIL_FROM=your_email@gmail.com
EMAIL_PASSWORD=your_app_password
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587

# CORS
ALLOWED_ORIGINS=http://localhost:3000

# Alembic
ALEMBIC_DATABASE_URL=${POSTGRES_URL}
